{
    "feature": {
        "use_features": [
            "app",
            "app_add",
            "app_te",
            "app_ce",
            "app_ohe_pca",
            "bur_ohe_sum",
            "bur_ohe_ratio",
            "bur_agg",
            "bbl_agg",
            "pcb_agg",
            "ccb_agg",
            "prv_agg",
            "inp_agg"
